{
  "expo": {
    "name": "ibi",
    "slug": "ibi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoIbi.png",
    "scheme": "ibi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/logoIbi.png",
        "backgroundImage": "./assets/logoIbi.png",
        "monochromeImage": "./assets/logoIbi.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/logoIbi.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logoIbi.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "YOUTUBE_API_KEY": "COLE_SUA_API_KEY_AQUI",
      "YOUTUBE_CHANNEL_ID": "UCxxxxxxxxxxxxxxxx" // opcional: pode deixar vazio e setar depois
    }
  }
}